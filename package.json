{
    "name": "hello-next-js",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "start": "next dev"
    },
    "dependencies": {
        "mobx": "^5.10.1",
        "mobx-react": "^6.0.3",
        "next": "^8.1.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "devDependencies": {
        "@babel/plugin-proposal-decorators": "^7.4.4",
        "babel-preset-react-app": "^9.0.0",
        "faker": "^4.1.0"
    },
    "babel": {
        "plugins": [
            [
                "@babel/plugin-proposal-decorators",
                {
                    "legacy": true
                }
            ]
        ],
        "presets": [
            [
                "@babel/preset-react",
                {
                    "pragma": "dom",
                    "pragmaFrag": "DomFrag",
                    "throwIfNamespace": false
                }
            ]
        ]
    }
}
